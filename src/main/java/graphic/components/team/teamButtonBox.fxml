<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<fx:root type="VBox" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1">
    <HBox>
        <Button fx:id="buttonRemove" mnemonicParsing="false" prefHeight="55.0" prefWidth="70.0" text="-" />
        <Button fx:id="buttonEdit" alignment="CENTER" mnemonicParsing="false" prefHeight="55.0" prefWidth="70.0" text="edit" onMouseClicked="#handleEditSelected" />
      <Button fx:id="buttonAppend" mnemonicParsing="false" prefHeight="55.0" prefWidth="70.0" text="+" onMouseClicked="#handleAppendEmployee"/>
    </HBox>
</fx:root>
