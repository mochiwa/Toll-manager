<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane id="mainPane" fx:id="mainPane" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1" fx:controller="graphic.form.employee.selectionMenu.TypeEmployeeMenuController">
    <columnConstraints>
        <ColumnConstraints hgrow="never" minWidth="10.0" />
        <ColumnConstraints hgrow="ALWAYS" maxWidth="-Infinity" minWidth="10.0" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="40.0" minHeight="40.0" prefHeight="40.0" vgrow="SOMETIMES" />
    </rowConstraints>
    <children>
        <Label text="Type" GridPane.columnIndex="0" GridPane.rowIndex="0" />
        <MenuButton fx:id="menu" maxWidth="Infinity" mnemonicParsing="false" text="MenuButton" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.valignment="CENTER" />
    </children>
</GridPane>
